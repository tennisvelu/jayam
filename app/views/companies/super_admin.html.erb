</table>

<center><table class="table">
<thead><tr>
<th>Company</th>
<th>Warehouse</th>
<TH>Rail Head</th>
<th>Direct Sale</th>
<th>Transhipment</th>
<th>Godown</th>
<th>Damaged</th>
<th>Total</th>
</tr></thead>
<%if @direct_sale.empty?%><br>
<tr><td>Sorry No Data</td></tr>
<%else%>
<%@direct_sale.map do |i|%>
<tr><td><%=Company.find(i[0]).company_name%></td>
<td><%=i[1]%></td>
<td><%=i[2]/20.to_f%>/<%=i[2]%></td>
<td><%=i[3]/20.to_f%>/<%=i[3]%></td>
<td><%=i[4]/20.to_f%>/<%=i[4]%></td>
<td><%=i[5]/20.to_f%>/<%=i[5]%></td>
<td><%=i[6]/20.to_f%>/<%=i[6]%></td>
<td><%=i[7]/20.to_f%>/<%=i[7]%></td>
<%end%></tr><%end%>
</table></center><br><br>

<center><table class="table">
<h5><b>Damaged</b>(Tons/Bags)</h5>
<%=form_for Damage.new ,:url=>{:controller=>"companies",:action=>"damage_date"} do |o|%>
<tr><%unless params[:to].present?%>
<td ><input name="from"  style="margin-left:5%;" class="form-control txtFrom" id="txtFrom" type="text" placeholder=<%=(Date.today).strftime("%Y-%m-01")%> required></td>
<td><input type="text" name="to" class="form-control txtTo" id="txtTo" placeholder=<%=Date.today.strftime("%Y-%m-%d")%> required></td>
<%else%>
<td ><input name="from"  style="margin-left:5%;" class="form-control txtFrom" id="txtFrom" type="text" placeholder=<%=params[:from]%> required></td>
<td><input type="text" name="to" class="form-control txtTo" id="txtTo" placeholder=<%=(Date.parse(params[:to])-1).strftime("%Y-%m-%d")%> required></td>
<%end%>
<td><%= o.submit "click", class:"btn btn-success"%></td></tr>
<%end%>
</table>

<table class="table">
<thead><tr>
<th>Company</th>
<th>Warehouse</th>
<th>Inward</th>
<th>Godown</th>
<th>Return</th>
<th>Total</th>
</tr></thead>
<%if @damage.empty?%><br>
<tr><td>Sorry No Data</td></tr>
<%else%>
<tbody>
<%@damage.map do |k|%>
<tr><td><%=Company.find(k[0]).company_name%></td>  
<td><%=k[1]%></td>
<td><%=k[2]/20.to_f%>/<%=k[2]%></td>
<td><%=k[3]/20.to_f%>/<%=k[3]%></td>
<td><%=k[4]/20.to_f%>/<%=k[4]%></td>
<%t=k[2]+k[3]+k[4]%>
<td><%=t/20.to_f%>/<%=t%></td>
<%end%></tr><%end%>
</tbody></table></center><br>
</html>

